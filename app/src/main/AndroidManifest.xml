<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kelvinferreira.geracaosaude">

    <application
        android:name="com.activeandroid.app.Application"
        android:allowBackup="true"
        android:icon="@mipmap/gs_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/gs_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="AA_DB_NAME"
            android:value="GS" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />
        <meta-data
            android:name="AA_MODELS"
            android:value="com.example.kelvinferreira.GeracaoSaude.model.Usuario, com.example.kelvinferreira.GeracaoSaude.model.Indices" />

        <activity android:name=".Login_01">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CadastroUsuarios" />
        <activity android:name=".CadastraIndices" />
        <activity android:name=".ExibirIndices" />
        <activity android:name=".IndicesAdapter"></activity>
    </application>

</manifest>